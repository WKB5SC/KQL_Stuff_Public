Syslog
| extend httpget = case(
    SyslogMessage has "httprequest", extract("get ([^-]+)", 1, SyslogMessage),
    ""
)
| extend clientip = case(
    SyslogMessage has "tcpconnstat", extract("- clientip ([^ ]+)", 1, SyslogMessage),
    ""
)
| where isnotempty(httpget) or isnotempty(clientip)
| project SyslogMessage, httpget, clientip