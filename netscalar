Syslog
| extend httpget = iff(startswith(SyslogMessage, "httprequest"), extract("get ([^-]+)", 1, SyslogMessage), "")
| extend clientip = iff(startswith(SyslogMessage, "tcpconnstat"), extract("- clientip ([^ ]+)", 1, SyslogMessage), "")
| where isnotempty(httpget) or isnotempty(clientip)
| project SyslogMessage, httpget, clientip